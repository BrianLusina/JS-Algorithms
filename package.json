{
  "name": "js-snippets",
  "version": "2.0.0",
  "private": true,
  "description": "Small JavaScript Algorithms, snippets and programs",
  "scripts": {
    "test": "ava src/**/*.spec.js --color",
    "test:watch": "ava --watch",
    "test:cover": "nyc ava",
    "send-cover-report": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianLusina/JS-Snippets.git"
  },
  "keywords": [
    "JavaScript",
    "JS-snippets",
    "JS-Algorithms",
    "ES6",
    "ES5",
    "ES2016"
  ],
  "author": "The Lusina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrianLusina/JS-Snippets/issues"
  },
  "homepage": "https://github.com/BrianLusina/JS-Snippets#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.1",
    "codecov": "^1.0.1",
    "expect": "^23.6.0",
    "istanbul": "^0.4.5",
    "nyc": "^13.0.1",
    "sinon": "^6.2.0",
    "xo": "^0.23.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js",
      "src/**/*.test.js"
    ],
    "sources": [
      "**/*.{js}",
      "!dist/**/*"
    ],
    "match": [
      "*oo",
      "!foo"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "verbose": true,
    "compileEnhancements": false,
    "require": [
      "esm"
    ]
  }
}
