{
  "name": "js-snippets",
  "version": "2.0.0",
  "private": true,
  "description": "Small JavaScript Algorithms, snippets and programs",
  "scripts": {
    "test-all": "jasmine-node tests/",
    "cover:istanbul": "istanbul cover jasmine-node --captureExceptions tests/",
    "cover:codacy": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec tests/**/*.js && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "send-cover-report":"cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianLusina/JS-Snippets.git"
  },
  "keywords": [
    "JavaScript",
    "JS-snippets",
    "JS-Algorithms",
    "ES6",
    "ES5",
    "ES2016"
  ],
  "author": "The Lusina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrianLusina/JS-Snippets/issues"
  },
  "homepage": "https://github.com/BrianLusina/JS-Snippets#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.1",
    "codecov": "^1.0.1",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "jasmine-node": "^1.14.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.6",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "standard": "^8.6.0"
  }
}
