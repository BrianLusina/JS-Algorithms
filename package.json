{
  "name": "jtscripts",
  "version": "3.0.0",
  "private": true,
  "description": "Small JavaScript & TypeScript Algorithms, DataStructures, Snippets and programs",
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch",
    "test:cover": "nyc ava",
    "send-cover-report": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "lint": "tsc --noEmit -p . && tslint \"*.ts?(x)\"",
    "lintci": "tslint \"*.ts?(x)\" --force",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianLusina/JS-Snippets.git"
  },
  "keywords": [
    "JavaScript",
    "JS-snippets",
    "JS-Algorithms",
    "ES6",
    "ES5",
    "ES2016"
  ],
  "author": "The Lusina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrianLusina/JS-Snippets/issues"
  },
  "homepage": "https://github.com/BrianLusina/JS-Snippets#readme",
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/jest": "^21.1.5",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "ava": "^3.13.0",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.8.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "expect": "^25.5.0",
    "istanbul": "^0.4.5",
    "jest": "^24.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "sinon": "^9.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "typescript": "^3.9.7",
    "xo": "^0.34.2"
  },
  "snyk": true,
  "dependencies": {
    "snyk": "^1.424.4"
  }
}
